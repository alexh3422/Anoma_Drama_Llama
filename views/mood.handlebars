<div class="column">
  <div id="userInfo" userId="{{user.id}}"><h2>{{user.username}}</h2></div>

  {{> mood-wheel}}

  <div class="allPosts">
    <h2>My Mood Tracker</h2>
    {{#each userPosts}}
    <div class="post-box">
      <p>You were feeling {{moodText}}</p>
      <p id="title">{{title}}</p>
      <p id="text">{{text}}</p>
      <p id="date">{{dateFormat createdAt}}</p>
    </div>
    {{/each}}
  </div>
</div>
</div>

<script src="/js/moodEntry.js"></script>